<!doctype html>
<html lang="fr-BE">
    <head>
        {# Meta #}
        <meta charset="UTF-8">
        <meta name="viewport"
              content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        {# Favicon #}
        <meta name="theme-color" content="#ffffff">
        <meta name="msapplication-TileColor" content="#531f09">
        <meta name="msapplication-config" content="{{ asset('/img/icons/browserconfig.xml') }}">
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('/img/icons/apple-touch-icon.png') }}">
        <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('/img/icons/favicon-32x32.png') }}">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('/img/icons/favicon-16x16.png') }}">
        <link rel="manifest" href="{{ asset('/img/icons/site.webmanifest') }}">
        <link rel="mask-icon" href="{{ asset('/img/icons/safari-pinned-tab.svg') }}" color="#5bbad5">
        <link rel="shortcut icon" href="{{ asset('/img/icons/favicon.ico') }}">

        <title>{% block title '' %} ~ Institut Labo &laquo; T &raquo;</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}

        <script src="{{ asset('dist/js/bootstrap.min.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
